  activate
  -- ROOT
  do script "cd ~/Downloads/distribution-lottery-app; printf \"\\033]0;ROOT\\007\"; echo \"# ã“ã“ã¯ãƒ«ãƒ¼ãƒˆä½œæ¥­ç”¨\""
  -- BACKEND
  do script "cd ~/Downloads/distribution-lottery-app/backend; printf \"\\033]0;BACKEND\\007\"; echo \"# backend èµ·å‹•: npm run dev\""
  -- FRONTEND
  do script "cd ~/Downloads/distribution-lottery-app/frontend; printf \"\\033]0;FRONTEND\\007\"; echo \"# frontend èµ·å‹•: npm start\""
end tell
EOF

echo "# å®Ÿè¡Œï¼ˆTerminalãŒ3ã¤ç«‹ã¡ä¸ŠãŒã‚Šã¾ã™ï¼‰"
